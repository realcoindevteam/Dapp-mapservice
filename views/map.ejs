<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA_Compatible" content="ie=edge">
    <title>Google Map Test</title>
    <style>
         html, body {
            height: 100%;
            font-family: "Lato", sans-serif;
        }

        #map {
            height: 90%;
            width: 100%;
        }
    </style>
</head>

<body>
    <h1>My Google Map</h1>
    <div id="map"></div>
    <script>
        function initMap() {
            // Map options
            var options = {
                zoom: 13,
                center: { lat: 37.5326, lng: 127.0246 }
            }


            // New map
            var map = new google.maps.Map(document.getElementById('map'), options);
            
                        // Add marker
                        var marker = new google.maps.Marker({
                            position:{lat:37.5326, lng:127.0246},
                            map:map,
                            icon:'https://developers.google.com/maps/documentation/javascript/examples/full/images/beachflag.png'
                        });
            
                        var infoWindow = new google.maps.InfoWindow({
                            content:'<h1>Lynn MA</h1>'
                        });
            
                        marker.addListener('click', function() {
                            infoWindow.open(map, marker);
                        });
                        

            // addMarker({lat:37.5326, lng:127.0246});
            // addMarker({lat:37.5318, lng:126.8223});

            // // Add Marker Function
            // function addMarker(coords) {
            //     var marker = new google.maps.Marker({
            //         position: coords,
            //         map: map,
            //         icon: 'https://developers.google.com/maps/documentation/javascript/examples/full/images/beachflag.png'
            //     });
            // }

        }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDscU4engo1Mwz73WPUNtQHUx6nXVg5nwE&callback=initMap">
    </script>
</body>

</html>